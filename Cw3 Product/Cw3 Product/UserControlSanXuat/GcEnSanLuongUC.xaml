<UserControl x:Class="Cw3_Product.UserControlSanXuat.GcEnSanLuongUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Cw3_Product.UserControlSanXuat"
             mc:Ignorable="d" 
             
             xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
             
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
            TextElement.Foreground="{DynamicResource MaterialDesignBody}"            
            TextElement.FontWeight="Medium"
            TextElement.FontSize="12"
             
             DataContext="{StaticResource EnPhieuSlVM}"
             
             d:DesignHeight="620" d:DesignWidth="1600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <WrapPanel Grid.Row="0" Orientation="Horizontal" Margin="10 5 0 5" HorizontalAlignment="Left">
            <Button Content="Tạo mới" Command="{Binding TaoDon}" VerticalAlignment="Center" Width="90" Margin="20 0 20 0" Background="Blue" Foreground="White"></Button>
            <Button Content="Xác nhận" Command="{Binding XacNhan}" VerticalAlignment="Center" Width="90" Margin="20 0 20 0" Background="Blue" Foreground="White">
                <Button.CommandParameter>
                    <MultiBinding Converter="{StaticResource myConverterEn}">
                        <MultiBinding.Bindings>
                            <Binding ElementName="PhieuSanLuong" />
                            <Binding ElementName="PhieuKiemTra"/>
                            <Binding ElementName="ThoiGianDungMay"/>
                            <Binding ElementName="PhieuNhietDo" />
                            <Binding ElementName="PhieuBaoDuong"/>
                            <Binding ElementName="PhieuThemNhua"/>
                        </MultiBinding.Bindings>
                    </MultiBinding>
                </Button.CommandParameter>
            </Button>
            <Button Content="Huỷ" Command="{Binding HuyDon}" VerticalAlignment="Center" Width="90" Margin="20 0 20 0" Background="Blue" Foreground="White">
                <Button.CommandParameter>
                    <MultiBinding Converter="{StaticResource myConverterEn}">
                        <MultiBinding.Bindings>
                            <Binding ElementName="PhieuSanLuong" />
                            <Binding ElementName="PhieuKiemTra"/>
                            <Binding ElementName="ThoiGianDungMay"/>
                            <Binding ElementName="PhieuNhietDo" />
                            <Binding ElementName="PhieuBaoDuong"/>
                            <Binding ElementName="PhieuThemNhua"/>
                        </MultiBinding.Bindings>
                    </MultiBinding>
                </Button.CommandParameter>
            </Button>
            <DatePicker SelectedDate="{Binding EnNgay, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay, StringFormat='dd/MM/yyyy>'}" IsEnabled="{Binding FlagNew}" materialDesign:HintAssist.Hint="Ngày làm" Width="180" materialDesign:TextFieldAssist.HasClearButton="True" Style="{StaticResource MaterialDesignFloatingHintDatePicker}" Margin="5 0 5 0" IsTodayHighlighted="True">
                <i:Interaction.Triggers>
                    <i:EventTrigger EventName="SelectedDateChanged">
                        <i:InvokeCommandAction Command="{Binding Endatechange}"/>
                    </i:EventTrigger>
                </i:Interaction.Triggers>
            </DatePicker>
            <ComboBox Text="{Binding EnCa}" IsEnabled="{Binding FlagNew}" VerticalAlignment="Center" materialDesign:HintAssist.Hint="Ca làm việc" Width="140" materialDesign:ComboBoxAssist.MaxLength="10" materialDesign:HintAssist.HintOpacity=".26" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Margin="5 0 5 0">
                <ComboBoxItem Content="Ngày"/>
                <ComboBoxItem Content="Đêm"/>
            </ComboBox>
            <ComboBox DisplayMemberPath="MaMay" ItemsSource="{Binding Mamaylist}" Text="{Binding EnMaMay}" IsEnabled="{Binding FlagNew}" IsEditable="True" VerticalAlignment="Center" materialDesign:HintAssist.Hint="Mã máy" Width="140" materialDesign:ComboBoxAssist.MaxLength="10" materialDesign:HintAssist.HintOpacity=".26" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Margin="5 0 5 0" IsTabStop="True"/>
            <ComboBox DisplayMemberPath="NhanVien" ItemsSource="{Binding NhanVienlist}" Text="{Binding EnNhanVien}" IsEnabled="{Binding FlagNew}" IsEditable="True" VerticalAlignment="Center" materialDesign:HintAssist.Hint="Nhân viên" Width="140" materialDesign:HintAssist.HintOpacity=".26" Style="{StaticResource MaterialDesignFloatingHintComboBox}" Margin="5 0 5 0" IsTabStop="True"/>
